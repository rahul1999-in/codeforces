#include<bits/stdc++.h>
#define lli long long int 
#define ll long long 
#define f(i, n) for(int i=0; i<n; i++)

using namespace std;

void test(){
	int a, b;
	cin>>a>>b;
	
	int side = min(max(a*2, b), max(a, 2*b));
	cout<<side*side<<endl;
}
int main(){
	
	int t;
	cin>>t;
	
	while(t--){
		test();
	}
	
	//test();
	return 0;

}





