#include<bits/stdc++.h>
#define lli long long int 
#define ll long long 
#define f(i, n) for(int i=0; i<n; i++)

using namespace std;



void test()
{
	
	int a, b;
	cin>>a>>b;
	
	if(a < b){
		cout<<a<<" " <<(b-a)/2;
	}
	else if(b < a){
		cout<<b<<" " <<(a-b)/2;
	
	}else{
		cout<<a<<" "<<0;
	}
	
	
}

int main(){
	
	
	test();
	
	return 0;

}
