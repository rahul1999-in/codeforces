	cin>>s;
	
	for(int i=1; i<=5; i++){
		cin>>str;
		if(s[0] == str[0] || s[0] == str[1]
		|| s[1] == str[0] || s[1] == str[1]){
			cout<<"yes"<<endl;
			return;
		}
	}
	cout<<"no"<<endl;
	
}
