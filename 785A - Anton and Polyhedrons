#include<bits/stdc++.h>
#define lli long long int 
#define ll long long 
#define f(i, n) for(int i=0; i<n; i++)

using namespace std;



void test()
{
	lli j;
	cin>>j;
	string s;
	int count=0;
	while(j--)
	{
		cin>>s;
		if(s == "Icosahedron")
		{
			count += 20;
		}
		else if(s == "Cube")
		{
		    count += 6;	
		}
		else if(s == "Octahedron")
		{
			count += 8;
		}
		else if(s == "Dodecahedron")
		{
			count += 12;
		}
		else{
			count += 4;
		}
	}
	cout<<count;
}

int main(){
	
	test();
	return 0;

}
